    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "gen": "graphql-codegen --config codegen.yml",
    "tsc": "tsc"
